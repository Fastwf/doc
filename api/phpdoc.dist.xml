<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="data/xsd/phpdoc.xsd"
>
    <title>Fast Web Framework</title>
    <paths>
        <output>api-doc</output>
    </paths>
    <version number="1.0.0">
        <folder>latest</folder>
        <api>
            <source dsn="vendor/fastwf/">
                <path>*/src/</path>
            </source>
            <ignore hidden="true" symlinks="true">
                <path>**/tests/**/*</path>
            </ignore>
            <extensions>
                <extension>php</extension>
            </extensions>
            <markers>
                <marker>TODO</marker>
                <marker>FIXME</marker>
            </markers>
            <ignore-tags>
                <ignore-tag>template</ignore-tag>
                <ignore-tag>template-extends</ignore-tag>
                <ignore-tag>template-implements</ignore-tag>
                <ignore-tag>extends</ignore-tag>
                <ignore-tag>implements</ignore-tag>
            </ignore-tags>
            <default-package-name>Fastwf</default-package-name>
        </api>
    </version>
    <template name="default"/>
</phpdocumentor>

